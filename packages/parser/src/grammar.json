          {
            "type": "PATTERN",
            "value": "((Data\\/|UserData\\/|Mods\\/)?([A-Z][A-z0-9 ]*\\.rte)(\\/[A-z0-9 ]*)*(\\/[A-z0-9 ]+\\.))"
          },
