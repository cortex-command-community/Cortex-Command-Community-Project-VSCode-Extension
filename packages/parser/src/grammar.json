    },
    "_integer": {
      "type": "PATTERN",
      "value": "(-?[0-9]+)"
    },
    "_float": {
      "type": "PATTERN",
      "value": "(-?[0-9]+\\.[0-9]+)"
    },
    "string": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": ".+"
        }
      }
    },
