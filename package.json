{
  "name": "cortex-command-community-project-language-support",
  "displayName": "Cortex Command Community Project Language Support",
  "description": "INI language support for development of content for the game Cortex Command",
  "icon": "icon.png",
  "version": "1.0.2",
  "publisher": "CortexCommandCommunityProject",
  "license": "LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/cortex-command-community/Cortex-Command-Community-Project-VSCode-Extension.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ccini",
        "aliases": [
          "INI (Cortex Command)",
          "ccini"
        ],
        "extensions": [
          ".ini"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ccini",
        "scopeName": "source.ccini",
        "path": "dist/syntaxes/ccini.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@nrwl/esbuild": "15.5.3",
    "@nrwl/eslint-plugin-nx": "15.5.3",
    "@nrwl/jest": "15.5.3",
    "@nrwl/linter": "15.5.3",
    "@nrwl/node": "15.5.3",
    "@nrwl/nx-cloud": "latest",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "28.1.1",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vscode/vsce": "^2.16.0",
    "esbuild": "^0.15.7",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^8.0.3",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "js-yaml": "^4.1.0",
    "nx": "15.5.3",
    "semantic-release": "^20.0.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "@nrwl/js": "15.5.3",
    "@nrwl/workspace": "15.5.3",
    "prettier": "^2.6.2",
    "typescript": "~4.8.2"
  },
  "scripts": {
    "prepare": "husky install"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}