{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "tasksRunnerOptions": {},
  "default": {
    "runner": "@nrwl/nx-cloud",
    "options": {
      "cacheableOperations": [
        "compile",
        "build",
        "lint",
        "test",
        "e2e",
        "watch"
      ]
    }
  },
  "defaultBase": "main",
  "targetDefaults": {
    "test": {
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/jest.preset.js"
      ]
    },
    "lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "watch": {
      "dependsOn": [
        "^watch"
      ]
    }
  },
  "workspaceLayout": {
    "libsDir": "packages"
  },
  "generators": {
    "@nrwl/web:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/web:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    }
  },
  "pluginsConfig": {},
  "nxCloudAccessToken": "OTgwZjc4MDctMmE1Zi00MDI1LWJmYTctMmJhODhhMDM0NWJlfHJlYWQtd3JpdGU="
}
